{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "attach",
            "name": "Attach",
            "pid": "${command:pickMyProcess}" // use ${command:pickProcess} to pick other users' processes
        },
        {
            "name": "Flutter: Attach to Device",
            "type": "dart",
            "request": "attach"
        },
        {
            "name": "Flutter: Windows",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "cwd": "${workspaceFolder}/flutter/realm_flutter/example",
            "args": ["-d", "windows"]
        },
        {
            "name": "Flutter: MacOS",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "cwd": "${workspaceFolder}/flutter/realm_flutter/example",
            "args": ["-d", "macos"]
        },
        {
            "name": "(Windows) Attach",
            "type": "cppvsdbg",
            "request": "attach",
            "processId": "${command:pickProcess}"
        },
        {
            "name": "Dart: Attach to Process",
            "type": "dart",
            "request": "attach",
            "vmServiceUri": "http://127.0.0.1:5858",
            "packages": "package:realm/main.dart"
        },
        {
            "name": "Dart: Debug",
            "type": "dart",
            "request": "launch",
            "program": "example/bin/myapp.dart",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Dart: Debug Test",
            "type": "dart",
            "request": "launch",
            "program": "test/realm_test.dart",
            "cwd": "${workspaceFolder}",
            "args": ["--name", ""]
        }
    ]
}